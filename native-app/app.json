    "splash": {
      "backgroundColor": "#fff",
      "resizeMode": "cover",
      "image": "./assets/splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackathon.votingiosdapp"
    },
    "android": {
      "package": "com.hackathon.votingandroiddapp"
    }
  }
