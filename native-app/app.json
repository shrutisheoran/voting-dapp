{
  "expo": {
    "name": "Voting DApp",
    "description": "This project is really great.",
    "slug": "vote-dapp",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
